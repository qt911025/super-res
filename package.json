{
  "name": "super-res2",
  "version": "0.0.2",
  "description": "RESTish wrapper for superagent, similar like angular-resource.",
  "scripts": {
    "prepare": "rollup -c",
    "test": "xo && ava",
    "coverage": "xo && nyc ava",
    "coverage-ci": "xo && nyc --reporter=lcovonly ava && codecov"
  },
  "files": [
    "src",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qt911025/super-res2.git"
  },
  "keywords": [
    "superagent",
    "resource",
    "angular",
    "rest"
  ],
  "contributors": [
    "Jeff Balboni <jbalboni@gmail.com>",
    "QuenTine <qt911025@qq.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qt911025/super-res2/issues"
  },
  "homepage": "https://github.com/qt911025/super-res2",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.24.0",
    "codecov": "^2.1.0",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.7.11",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "sinon": "^2.1.0",
    "xo": "^0.18.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "cache-manager": "^2.4.0",
    "methods": "^1.1.2",
    "superagent": "^3.5.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "failFast": true
  },
  "xo": {
    "space": true,
    "rules": {
      "ava/no-ignored-test-files": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": [
            "!src/**/*.*"
          ]
        }
      ]
    },
    "parser": "babel-eslint"
  },
  "precommit": [
    "coverage"
  ]
}
