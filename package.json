{
  "name": "super-res2",
  "version": "0.0.3",
  "description": "RESTish wrapper for superagent, similar like angular-resource.",
  "scripts": {
    "prepare": "rollup -c",
    "test": "yarn run lint && ava",
    "coverage": "yarn run lint && nyc ava",
    "coverage-ci": "yarn run lint && nyc --reporter=lcovonly ava && codecov",
    "lint": "eslint src test"
  },
  "files": [
    "src",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/qt911025/super-res2.git"
  },
  "keywords": [
    "superagent",
    "resource",
    "angular",
    "rest"
  ],
  "contributors": [
    "Jeff Balboni <jbalboni@gmail.com>",
    "QuenTine <qt911025@qq.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qt911025/super-res2/issues"
  },
  "homepage": "https://github.com/qt911025/super-res2",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "ava": "^1.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "codecov": "^4.0.0-0",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nyc": "^13.3.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.0",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "sinon": "^7.2.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "cache-manager": "^2.9.0",
    "methods": "^1.1.2",
    "superagent": "~4.1.0"
  },
  "peerDependencies": {
    "cache-manager": "~2.9.0"
  },
  "engines": {
    "node": "^6.12.3 || ^8.9.4 || >=10.0.0"
  },
  "ava": {
    "files": [
      "test/unit/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "failFast": true
  },
  "precommit": [
    "coverage"
  ]
}